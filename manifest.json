{
  "manifest_version": 3,
  "name": "Tab Smasher",
  "version": "1.0",
  "developer": {
    "name": "Matt Foster",
    "url": "https://hackerific.net"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
